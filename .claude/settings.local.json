      "Bash(npm run build:*)",
      "Bash(timeout 10 npm run dev)",
      "mcp__ide__getDiagnostics",
      "Bash(git add:*)",
      "Bash(mv:*)",
      "Bash(move compare.tsx compare-old.tsx)",
