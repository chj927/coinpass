      "Bash(done)",
      "Bash(rm:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git checkout:*)",
      "Bash(find:*)",
      "Bash(cmd /c:*)",
      "Bash(powershell:*)",
      "Bash(dir:*)",
      "Bash(npx tsc:*)",
      "Bash(npm run lint)",
      "Bash(psql:*)"
    ],
    "deny": []
  }
}