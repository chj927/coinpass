{
  "name": "coinpass",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "node setup-security.js",
    "dev": "vite",
    "dev:server": "node server/api-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "node server/api-server.js",
    "start:client": "vite preview --host 0.0.0.0",
    "lint": "tsc --noEmit",
    "security:check": "npm audit",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "concurrently": "^9.2.0",
    "tsx": "^4.20.4",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
